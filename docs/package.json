{
  "name": "docs",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && rm -rf .output/public/__tsr && cp -R .output/dist/client/__tsr .output/public/__tsr && cp .output/public/_shell.html .output/public/index.html",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "start": "serve .output/public --config ../../serve.json",
